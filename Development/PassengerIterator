CONTRACT_TYPE
{
//REQUIREMENTS FOR CONTRACT TO APPEAR


//DATA NODES TO PROCESS FOR CONTRACT USE

	DATA
	{
		type = List<Kerbal>
		kerbals = [ NewKerbal(), NewKerbal(), NewKerbal(), NewKerbal() ].Random(2,4)
	}

//CONTRACT DESCRIPTION
	name = BugTest
	title = BugTest #395

	description = Description of bug.
	synopsis = Test the bug.
	completedMessage = You have tested the bug.
	
	maxSimultaneous = 1
	
	advanceFunds = 1000000
	rewardScience = 100000
	rewardReputation = 10000


//BEHAVIORS TO DO WHEN CREATING CONTRACT	

	BEHAVIOUR
	{
		name = SpawnPassengers
		type = SpawnPassengers
		
		kerbal = @/kerbals
		kerbalType = Tourist
		
	}


//PARAMETERS FOR CONTRACT COMPLETION
	PARAMETER
	{
		name = HasPassengers
		type = HasPassengers
		title = Passenger: @kerbal.First()
		hideChildren = true
		
		ITERATOR
		{
			type = Kerbal
			kerbal = @/kerbals
		}
	}


	PARAMETER
	{
		name = ReachState
		type = ReachState
		title = Get moving
		situation = LANDED
		minSpeed = 5.0
		targetBody = Kerbin
		
	}
	
}
